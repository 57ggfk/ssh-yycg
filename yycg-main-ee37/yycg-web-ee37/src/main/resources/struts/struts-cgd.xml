<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
<!-- 药品目录管理 -->
	<package name="cgd" namespace="/cgd" extends="base">
		<action name="*" class="yycgdAction" method="{1}">
		<!-- 导出药品信息 -->
			<result name="add">/business/cgd/add.jsp</result>
			<result name="edit">/business/cgd/edit.jsp</result>
			<result name="list">/business/cgd/list.jsp</result>
			<result name="addyycgdmx">/business/cgd/addyycgdmx.jsp</result>
			<result name="dispose">/business/cgd/dispose.jsp</result>
			<result name="disposelist">/business/cgd/disposelist.jsp</result>
			<result name="checklist">/business/cgd/checklist.jsp</result>
			<result name="viewyycgdmx">/business/cgd/viewyycgdmx.jsp</result>
			<result name="check">/business/cgd/check.jsp</result>
		</action>
	</package>
</struts>